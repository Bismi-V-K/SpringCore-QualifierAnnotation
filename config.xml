<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">
        
    <context:annotation-config/>
    <bean id="humanheart" class="com.bis.mi.SpringCore_qualifierannotation.Heart">
        <property name="number" value="2"></property>
    </bean>
    <bean id="octopusheart" class="com.bis.mi.SpringCore_qualifierannotation.Heart">
        <property name="number" value="3"></property>
    </bean>
    <bean id="human" class="com.bis.mi.SpringCore_qualifierannotation.Human"></bean>
</beans>
